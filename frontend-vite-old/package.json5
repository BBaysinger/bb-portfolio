{
  // Because I often need comments in my package list
  // (It's an extra step but the absence of comments is infuriating...)
  // [Kinda hard to keep up with this, honestly.]
  // TODO: Create a script to generate this file from package.json
  "name": "bb-portfolio-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "description": "Online F/E developer portfolio built in React.",
  "author": "Bradley Baysinger",
  "license": "MIT",
  "scripts": {
    // Pretty build. Run before a commit. Ensures prettier hasn't broken anything, which it can.
    "pb": "prettier --write . && tsc -b && vite build",
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "npm run build && vite preview",
    "format": "prettier --write .",
    "deploy": "vite build && npx netlify deploy --dir=dist --prod",
    "prepare-package": "node -e \"require('fs').writeFileSync('./package.json', JSON.stringify(require('json5').parse(require('fs').readFileSync('./package.json5', 'utf8')), null, 2))\""
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.8.2",
    "@vitejs/plugin-react-swc": "^3.10.2",
    "bootstrap": "^5.3.7",
    "crypto-js": "^4.2.0",
    "exenv": "^1.2.2",
    "gsap": "^3.13.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-icons": "^5.5.0",
    "react-redux": "^9.2.0",
    "react-router": "^7.6.3",
    "react-router-dom": "^7.6.3",
    // Vite 7.0.0 breaks on Netlify, so use previous version, 6.3.5
    "vite": "^6.3.5",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@swc/core": "^1.12.11",
    "@types/crypto-js": "^4.2.2",
    "@types/exenv": "^1.2.2",
    "@types/node": "^24.0.12",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/react-scroll": "^1.8.10",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "json5": "^2.2.3",
    "prettier": "^3.6.2",
    "rollup": "^4.44.2",
    "sass": "^1.89.2",
    "typescript": "~5.8.3"
  },
  "engines": {
