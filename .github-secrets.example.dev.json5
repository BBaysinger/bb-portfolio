// Development environment GitHub secrets (ENV_PROFILE=dev)
// --------------------------------------------------------
// - Sync with: npm run sync:secrets:dev (targets GitHub environment "dev")
// - Store the real values in .github-secrets.private.dev.json5 (gitignored)
// - Keys are unprefixed; the environment file determines which GitHub env receives them
{
  strings: {
    // Region + infra config
    AWS_REGION: "us-west-2",
    S3_BUCKET: "bb-portfolio-media-dev",

    // MongoDB Atlas connection string for dev content
    MONGODB_URI: "mongodb+srv://username:password@cluster0.example.mongodb.net/bb-portfolio-dev?retryWrites=true&w=majority&appName=bb-portfolio-dev",

    // Payload CMS secret for dev (64 hex chars)
    PAYLOAD_SECRET: "1234567890abcdef1234567890abcdef1234567890abcdef1234567890abcdef",

    // SMTP / SES email routing for admin + contact flows
    SES_FROM_EMAIL: "noreply@dev.bbaysinger.com",
    SES_TO_EMAIL: "contact@bbaysinger.com",
    SMTP_FROM_EMAIL: "noreply@dev.bbaysinger.com",

    // Frontend origins used for CORS/CSRF guards on dev backend
    FRONTEND_URL: "https://dev.bbaysinger.com,http://203.0.113.10:4000",

    // Internal service URL (Docker Compose service name + port)
    BACKEND_INTERNAL_URL: "http://bb-portfolio-backend-dev:3000",
  },

  files: {},
}
